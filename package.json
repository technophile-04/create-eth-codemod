{
  "name": "create-eth-codemod",
  "version": "0.1.0",
  "description": "Codemods for create-eth extensions",
  "type": "module",
  "bin": {
    "create-eth-codemod": "bin/create-eth-codemod.js",
    "migrate-scaffold-ui-imports": "bin/migrate-scaffold-ui-imports.js"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "migrate-scaffold-ui": "node dist/migrate-scaffold-ui-imports.js",
    "changeset": "changeset",
    "changeset:release": "yarn build && changeset publish"
  },
  "keywords": [
    "cli",
    "codemod",
    "scaffold-eth",
    "create-eth"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "arg": "^5.0.2",
    "chalk": "^5.4.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@eslint/js": "^9.31.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^20.19.8",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "prettier": "^3.6.2",
    "rollup": "^4.45.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "packageManager": "yarn@3.5.0"
}
